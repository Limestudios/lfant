<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="networktest" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Default">
				<Option output="$(DIR_SFE)/bin64/$(PLATFORM)/networktest" prefix_auto="1" extension_auto="1" />
				<Option object_output="$(TARGET_OUTPUT_DIR)/build/$(PROJECT_NAME)" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="$(DIR_SFE)/src/engine" />
			<Add directory="$(DIR_SFE)/src/external/glm" />
			<Add directory="$(DIR_SFE)/src/external/boost" />
			<Add directory="$(DIR_SFE)/src/external/raknet/Source" />
		</Compiler>
		<Linker>
			<Add library="sfengine" />
			<Add library="raknet" />
			<Add library="bullet" />
			<Add directory="$(TARGET_OUTPUT_DIR)" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
